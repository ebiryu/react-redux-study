{
  "env": {
    "browser": true,
    "es6": true,
    "amd": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": "off",
    "indent": ["error", 2],
    "react/jsx-uses-vars": 1
  },
  "globals": {
    "exports": true,
    "process": true,
    "module": true
  }
}
